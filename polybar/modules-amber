[module/tube]
type = custom/ipc
hook-0 = echo
hook-1 = nohup qutebrowser -R --target window youtube.com.br &> /dev/null &
hook-2 = nohup wq mpy &> /dev/null &
initial = 1
click-left = polybar-msg -p %pid% hook tube 2
click-right = polybar-msg -p %pid% hook tube 3
format = %{T4}%{F T-}
format-padding = 1

