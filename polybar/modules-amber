[module/mp]
type = custom/menu
expand-right = false
format-suffix = "%{T6}    %{T-}"
menu-0-0 = "%{T6}    %{T-}"
menu-0-0-exec = mp $(wq app.paste) &>/dev/null &
menu-0-1 = "%{T6}    %{T-}"
menu-0-1-exec = mp plistyad null &>/dev/null &
menu-0-2 = "%{T6}    %{T-}"
menu-0-2-exec = mp save &>/dev/null &
menu-0-3 = "%{T6}    %{T-}"
menu-0-3-exec = mp stop &>/dev/null &
menu-0-4 = "%{T6}    %{T-}"
menu-0-4-exec = mp select.file &>/dev/null &
menu-0-5 = "%{T6}    %{T-}"
menu-0-5-exec = mp prev &>/dev/null &
menu-0-6 = "%{T6}    %{T-}"
menu-0-6-exec = mp pause.toggle &>/dev/null &
menu-0-7 = "%{T6}    %{T-}"
menu-0-7-exec = mp next &>/dev/null &

label-open = "%{T6}    %{T-}" 
label-close = "%{T6}    %{T-}"
label-separator = |

format-padding = 1
format-background = ${colors.background}
format-foreground = ${colors.foreground}
format-underline = ${colors.color6}
format-overline = ${colors.color6}