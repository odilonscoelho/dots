[module/xkeyboard]
type = internal/xkeyboard
; List of indicators to ignore
format = "<label-layout> <label-indicator>"
format-spacing = 1
format-foreground = ${colors.foreground}
format-background = ${colors.background}
format-underline = ${colors.color6}
format-overline = ${colors.color6}
layout-icon-0 = pt_BR;P
layout-icon-1 = en_US;U
label-layout = "%{T3}  %{T-} %layout%"
label-layout-padding = 1
label-indicator = "%name%"
label-indicator-padding = 1
label-indicator-on-foreground = ${colors.background}
label-indicator-on-background = ${colors.color6}
label-indicator-on = " %name% "
click-left = setxkbmap -model abnt2 -layout br -variant abnt2
click-right = setxkbmap -model us -layout us

[module/title]
type = internal/xwindow
; Available tags:
;   <label> (default)
format = <label>
format-background = ${colors.background}
format-foreground = ${colors.foreground}
format-underline = ${colors.color6}
format-overline = ${colors.color6}
format-padding = 4

; Available tokens:
;   %title%
; Default: %title%
label = %title%
label-maxlen = 50

; Used instead of label when there is no window title
; Available tokens:
;   None
label-empty = Empty
label-empty-foreground = ${colors.foreground}

[module/close]
type = custom/ipc
hook-0 = echo "%{T4}"
initial = 1
click-left = bspc node -c
click-right = bspc node -k
scroll-up =$(wq bsp.clearmodifiers && wq notificatime 5000 "ClearModifiers...")
scroll-down = $(wq bsp.clearmodifiers && wq notificatime 5000 "ClearModifiers...")
format-padding = 0
format-background = ${colors.background}
format-foreground = #FF815B
;format-underline = ${colors.color6}
;format-overline = ${colors.color6}
tail = true

[module/floating]
type = custom/ipc
hook-0 = echo "%{T4}"
initial = 1
click-left = bspc node -t floating
click-right = bspc node -t tiled
scroll-up =$(wq notificatime 900000 "$(wq tradutor.sel)")
scroll-down = $(wq msg "$(wq tradutor.sel)")
format-padding = 0
format-background = ${colors.background}
format-foreground = #C0FD86
;format-underline = ${colors.color6}
;format-overline = ${colors.color6}
tail = true

[module/cmd]
